{
  "name": "jenkins-client",
  "version": "0.0.1",
  "description": "a small client for jenkins",
  "main": "index.js",
  "files": [
    "LICENSE",
    "README.md",
    "index.js"
  ],
  "scripts": {
    "test": "mocha test.js",
    "coverage": "istanbul --include-all-sources cover _mocha",
    "coverage:check": "npm run coverage && istanbul check-coverage",
    "coverage:open": "open reports/index.html",
    "lint": "jshint -c .jshintrc *.js",
    "style": "jscs *.js",
    "complexity": "plato -r -t 'jenkins-client code report' -l .jshintrc -x 'node_modules|reports|test' -d reports/plato .",
    "complexity:open": "open reports/plato/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joaquimserafim/jenkins-client.git"
  },
  "keywords": [
    "jenkins",
    "client",
    "http",
    "utils",
    "continuous integration"
  ],
  "author": "@joaquimserafim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaquimserafim/jenkins-client/issues"
  },
  "homepage": "https://github.com/joaquimserafim/jenkins-client",
  "devDependencies": {
    "chai": "^3.5.0",
    "istanbul": "^0.4.2",
    "jscs": "^2.9.0",
    "jshint": "^2.9.1",
    "mocha": "^2.4.5",
    "nock": "^7.0.2",
    "plato": "^1.5.0",
    "pre-commit": "^1.1.2"
  },
  "dependencies": {
    "superagent": "^1.7.2"
  },
  "pre-commit": [
    "lint",
    "style",
    "test",
    "coverage:check"
  ]
}
